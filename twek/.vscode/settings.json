  "tailwindCSS.experimental.classRegex": [
    "class\\s*=\\s*[\"']([^\"']*)[\"']",
    "className\\s*=\\s*[\"']([^\"']*)[\"']"
  ],
  "css.validate": false,
  "scss.validate": false,
  "less.validate": false
}
